{
    "log": {
//        "access": "/var/log/xray/access.log",
//        "error": "/var/log/xray/error.log",
//        "dnsLog": false,
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "port": 443,
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "youruuuuuuuuuuuuuid",
                        "flow": "xtls-rprx-direct",
                        "level": 0,
                        "email": "main@user.io"
                    },
                    {
                        "id": "haproooooooooooxyid",
                        "flow": "xtls-rprx-direct"
                        "level": 0,
                        "email": "haproxy@openwrt.io",
                    }
                ],
                "decryption": "none",
                "fallbacks": [
                    {
                        "dest": 1310,
                        "xver": 1
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "xtls",
                "xtlsSettings": {
                    "serverName": "yourservername",
                    "fingerprint": "randomized",
//                    "allowInsecure": false,
//                    "alpn": ["http/1.1"],
                    "alpn": ["h2", "http/1.1"],
                    "certificates": [
                        {
                            "certificateFile": "/usr/local/nginx/conf/ssl/yourservername/fullchain.cer",
                            "keyFile": "/usr/local/nginx/conf/ssl/yourservername/yourservername.key"
                        }
                    ]
                }
            }
        },
        {
            "port": 1310,
            "listen": "127.0.0.1",
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password": "passsssssssword",
                        "level": 0,
                        "email": "trojan@user.io"
                    }
                ],
                "fallbacks": [
                    {
                        "dest": 80
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "none",
                "tcpSettings": {
                    "acceptProxyProtocol": true
                }
            }
//        },
//        {
//            "port": 2021,
